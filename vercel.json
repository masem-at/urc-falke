{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "turbo run build --filter=@urc-falke/web",
  "devCommand": "turbo run dev --filter=@urc-falke/web",
  "installCommand": "pnpm install",
  "framework": null,
  "outputDirectory": "apps/web/dist",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./apps/web ./packages/shared ./packages/ui",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://urc-falke-api.vercel.app/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
