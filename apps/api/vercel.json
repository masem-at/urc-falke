{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "urc-falke-api",
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
